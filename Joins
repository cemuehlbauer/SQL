--Database 1

CREATE TABLE shipments (
    sender_id INTEGER,
    shipment_id INTEGER,
    package_description TEXT,
    weight_kg REAL,
    shipping_method TEXT,
    shipping_date DATE,
    delivery_date DATE,
    status TEXT
);

